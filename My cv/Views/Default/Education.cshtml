
@using My_cv.Models.Entity
@model List<Table_educations>


<div class="resume-section-content">
    <h2 class="mb-5">Eğitim Hayatım</h2>
    @foreach (var schools in Model)
    {
    <div class="d-flex flex-column flex-md-row justify-content-between mb-5">
        <div class="flex-grow-1">
            <h3 class="mb-0">@schools.Title</h3>
            <div class="subheading mb-3">@schools.Subtitle_0</div>
            <div>@schools.Subtitle_1</div>
        </div>
        <div class="flex-shrink-0"><span class="text-primary">@schools.Date</span></div>
    </div>
    }
</div>
